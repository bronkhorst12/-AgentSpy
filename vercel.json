{
    "buildCommand": "pnpm install --prefer-offline && pnpm build",
    "devCommand": "pnpm dev",
    "installCommand": "pnpm install --prefer-offline",
    "framework": "vite",
    "outputDirectory": "dist",
    "redirects": [
        {
            "source": "/",
            "destination": "/whale-activity"
        }
    ],
    "rewrites": [
        {
            "source": "/:path*",
            "destination": "/index.html"
        }
    ]
}