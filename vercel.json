{
    "buildCommand": "npm run build",
    "devCommand": "npm run dev",
    "installCommand": "npm ci --legacy-peer-deps",
    "framework": "vite",
    "outputDirectory": "dist",
    "redirects": [
        {
            "source": "/",
            "destination": "/whale-activity"
        }
    ],
    "rewrites": [
        {
            "source": "/:path*",
            "destination": "/index.html"
        }
    ]
}